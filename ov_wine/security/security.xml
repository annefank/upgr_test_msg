<odoo>
    <record model="ir.module.category" id="module_ov_wine">
        <field name="name">ov wine</field>
        <field name="sequence">5</field>
    </record>
    <record id="group_ov_wine_manager" model="res.groups">
        <field name="name">ov_wine admin</field>
        <field name="implied_ids" eval="[(4,ref('base.group_user'))]"/>
        <field name="category_id" ref="module_ov_wine"/>
        <field name="comment">Admin user</field>
    </record>
    <record id="group_ov_wine_user" model="res.groups">
        <field name="name">ov_wine user</field>
        <field name="category_id" ref="module_ov_wine"/>
        <field name="comment">Normal user</field>
    </record>
</odoo>
