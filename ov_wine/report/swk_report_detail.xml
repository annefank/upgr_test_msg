<odoo>

        <report id="ov_wine.swk_report_template_detail_report"
            name="ov_wine.swk_report_detail_template"
            file="ov_wine.swk_report_detail_template"
            model="product.product"
            report_type="qweb-pdf"
            string="CSCV-SWK Detail Inventar Formular A"
            menu="True"/>

    <template id="ov_wine.swk_report_detail_template">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <div class="page">

                    <table class="table table-condensed table-bordered" style="width: 100%">
                        <thead>
                            <th class="text-center" style="width: 10%">SWK ID</th>
                            <th class="text-left" style="width: 10%">Weintype</th>
                            <th class="text-center" style="width: 15%">Name</th>
                            <th class="text-center" style="width: 15%">Anzahl</th>
                            <th class="text-center" style="width: 15%">Flaschengr√∂sse</th>
                            <th class="text-center" style="width: 15%">Total l</th>

                        </thead>
                        <tbody>
                            <!--<t t-foreach="docs.sorted(key=lambda r: (r.swk_city_id.name, r.winetype_id.name))"-->
                            <t t-foreach="docs.sorted(key=lambda r: (r.winetype_id, r.swk_city_id.name))"
                               t-as="doc">
                                <t t-set="qty" t-value="doc['qty_available']"/>
                                <t t-set="vol" t-value="doc.uom_id.factor"/>
                                <t t-set="liter" t-value="qty*vol"/>
                                <t t-set="winetype" t-value="doc.winetype_id.name"/>
                                <t t-set="SWK_City" t-value="doc.swk_city_id.name"/>


                                <tr>
                                    <td class="text-left">
                                        <span t-esc="doc.swk_city_id.name"/>
                                    </td>
                                    <td class="text-left">
                                        <span t-esc="doc.winetype_id.name"/>
                                    </td>
                                    <td class="text-left">
                                        <span t-esc="doc.name"/>
                                    </td>
                                    <td class="text-left">
                                        <span t-esc="doc['qty_available']"/>
                                    </td>
                                    <td class="text-left">
                                        <span t-esc="doc.uom_id.factor"/>
                                    </td>
                                    <td class="text-center">
                                        <span t-esc="liter"/>
                                    </td>
                                </tr>
                            </t> <!-- for each -->


                        </tbody>
                    </table>
                </div>
            </t>
        </t>
    </template>
</odoo>